<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PopulateCache async="false" continueOnError="false" enabled="true" name="PopulateCacheSaveSFAccessToken">
    <DisplayName>PopulateCache.SaveSFAccessToken</DisplayName>
    <FaultRules/>
    <Properties/>
    <CacheKey>
      <Prefix>sfdc</Prefix>
      <KeyFragment>access-token</KeyFragment>
    </CacheKey>
  	<CacheResource>sf-token-cache</CacheResource>
    <Scope>Exclusive</Scope>
    <ExpirySettings>
        <TimeoutInSec>300</TimeoutInSec>
    </ExpirySettings>
    <Source>sfdc.access-token</Source>
</PopulateCache>
